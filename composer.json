{
    "name": "webriqcms/multisite",
    "type": "webriqcms-modules",
    "description": "Multisite modules for WebriQ CMS",
    "license": "BSD-3-Clause",
    "keywords": [
        "webriqcms",
        "module",
        "multisite"
    ],
    "homepage": "http://webriq.com/",
    "authors": [
        {
            "name": "WebriQ Dev Team",
            "email": "info@webriq.com",
            "homepage": "http://webriq.com/",
            "role": "Development Team"
        }
    ],
    "support": {
        "email": "info@webriq.com",
        "source": "https://github.com/webriq/multisite"
    },
    "require": {
        "php": ">=5.4",
        "webriqcms/module-installer": "*",
        "webriqcms/core": "*"
    },
    "autoload": {
        "psr-0": {
            "Grid\\DomainManager": "module/DomainManager/src/",
            "Grid\\MultisiteCentral": "module/MultisiteCentral/src/",
            "Grid\\MultisitePlatform": "module/MultisitePlatform/src/"
        },
        "classmap": [
            "module/DomainManager/Module.php",
            "module/MultisiteCentral/Module.php",
            "module/MultisitePlatform/Module.php"
        ]
    },
    "extra": {
        "display-icon": {
            "63": "https://raw.github.com/webriqcms/multisite/%reference%/icons/63x63.png"
        },
        "display-name": {
            "en": "Multisite & Domain Manager"
        },
        "display-description": {
            "en": "A. The Multisite Function allows you to create multiple websites instead of just one. It is ideal for those who want to operate several websites on one server.\nB. The Domain Manager Function allows you to allocate a top level domain (TLD) to your WebriQ CMS website."
        },
        "patch-classes": {
            "Grid\\MultisitePlatform\\Installer\\Patch": "module/MultisitePlatform/src/Grid/MultisitePlatform/Installer/Patch.php"
        },
        "module": "Grid\\DomainManager"
    }
}
